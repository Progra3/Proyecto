<!doctype html>
<html lang="en">
@Html.Partial("~/Views/Shared/_PartialHeader.cshtml")
<body>
    <div class="wrapper">
        @Html.Partial("~/Views/Shared/_PartialAside.cshtml")
        <div class="main-panel">
            <nav class="navbar navbar-transparent navbar-absolute">
                @Html.Partial("~/Views/Shared/_PartialMenu.cshtml")
            </nav>
            <div class="content">
                <div class="container-fluid">
                    @RenderBody()

                </div>
            @Html.Partial("~/Views/Shared/_PartialFooter.cshtml")
            </div>
        </div>
        </div> 
</body>
<!--   Core JS Files   -->
<script src="~/Tema/assets/js/jquery-3.1.0.min.js" type="text/javascript"></script>
<script src="~/Tema/assets/js/bootstrap.min.js" type="text/javascript"></script>
<script src="~/Tema/assets/js/material.min.js" type="text/javascript"></script>
<!--  Charts Plugin -->
<script src="~/Tema/assets/js/chartist.min.js"></script>
<!--  Notifications Plugin    -->
<script src="~/Tema/assets/js/bootstrap-notify.js"></script>
<!--  Google Maps Plugin    -->
<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js"></script>
<!-- Material Dashboard javascript methods -->
<script src="~/Tema/assets/js/material-dashboard.js"></script>
<!-- Material Dashboard DEMO methods, don't include it in your project! -->
<script src="~/Tema/assets/js/demo.js"></script>
<script type="text/javascript">
    $(document).ready(function () {

        // Javascript method's body can be found in assets/js/demos.js
        demo.initDashboardPageCharts();

    });
</script>
@RenderSection("scripts", required: false)
</html>









